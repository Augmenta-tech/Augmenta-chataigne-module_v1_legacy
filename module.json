{
	"name":"Augmenta v1",
	"type":"OSC",
	"path":"Theoriz",
	
	"version":"1.1.0",
	"description":"Get tracking data from THEORIZ's Augmenta tracking system v1",
	"url":"https://github.com/Theoriz/Augmenta-chataigne-module_v1",
	"downloadURL":"https://github.com/Theoriz/Augmenta-chataigne-module_v1/archive/master.zip",
	
	"hasInput":true,
	"hasOutput":false,
	"hideDefaultCommands":true,
	"hideDefaultParameters": ["autoRange", "autoFeedback"],

	"parameters":
	{
		"Single Person Mode":
		{
			"type":"Enum",
			"options":
			{
				"None":"none",
				"Oldest":"oldest",
				"Newest":"newest"
			}
		},
		"Protocol Version":
		{
			"type":"Enum",
			"options":
			{
				"v2":"v2",
				"v1":"v1"
			}
		}
	},

	"defaults":
	{		
		"OSC Input":
		{
			"localPort":12000,
			"local":false,
		}	
	},

	"values":
	{
		
		"Scene":
		{
			"type":"Container",
		},
		"Single Person":
		{
			"type":"Container",

			"hasData":
			{
				"type":"Boolean",
				"ReadOnly":true,
			},
		}
	},

	"scripts":
	[
		"Augmenta.js"
	]

}
